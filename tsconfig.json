  "compilerOptions": {
    "target": "ES2023",
    "module": "ES2022",
    "moduleResolution": "node",
    "esModuleInterop": true,
    "outDir": ".",
    "types": ["node"],
    "baseUrl": ".",
    "paths": {
      "@/*": ["/*"]
    },
    "forceConsistentCasingInFileNames": true,
    "strict": true,
  }
}